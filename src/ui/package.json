  "scripts": {
    "build_deps": "concurrently -m 1 -c gray.dim,green.dim,magenta.dim,yellow.dim -n clean,components,api,api-react \"rimraf packages/*/dist\" \"yarn workspace @pixie-labs/components build\" \"yarn workspace @pixie-labs/api build\" \"yarn workspace @pixie-labs/api-react build\"",
    "build_dev": "webpack --mode=development --stats=verbose --progress",
    "build_prod": "concurrently -m 1 \"rimraf dist\" \"yarn build_deps\" \"webpack --mode=production\"",
    "clean": "rimraf dist packages/*/dist",
    "dev": "concurrently -c green.dim,magenta.dim,yellow.dim,cyan.dim -n components,api,api-react,ui \"yarn workspace @pixie-labs/components start\" \"yarn workspace @pixie-labs/api start\" \"yarn workspace @pixie-labs/api-react start\" \"webpack serve --mode=development --hot\"",
    "test": "jest",
    "test_ci": "jest --maxWorkers=4",
    "coverage": "jest --coverage",
    "coverage_ci": "jest --coverage --maxWorkers=4",
    "license_check": "./node_modules/license-checker/bin/license-checker",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}'",
    "vgspec": "NODE_PATH=./src ts-node --compiler-options '{\"module\":\"commonjs\"}' src/utils/print-vgspec-cli.ts"
  },
